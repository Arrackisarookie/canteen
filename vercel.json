{
  "devCommand": "uvicorn canteen.main:app --host 0.0.0.0 --port 8000",
  "builds": [
    {
      "src": "canteen/main.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "canteen/main.py"
    }
  ]
}